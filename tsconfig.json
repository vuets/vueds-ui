{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": false,
    "preserveConstEnums": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "removeComments": false,
    "outDir": "lib"
  },
  "compileOnSave": false,
  "files": [
    "src/index.ts",
    "src/trie.ts",
    "src/util.ts",
    "src/dom_util.ts",
    "src/pager_util.ts",
    "src/vm_util.ts",
    "src/keymage.ts",
    "src/v2/index.ts",
    "src/d/disable.ts",
    "src/d2/disable.ts",
    "src/d/pclass.ts",
    "src/d2/pclass.ts",
    "src/d/pager.ts",
    "src/gen/common.ts",
    "src/tpl/common.ts",
    "src/tpl/legacy/pager_controls.ts",
    "src/tpl/legacy/list.ts"
  ]
}
